<%- include('header/head.ejs') %>
<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <%- include('header/sidebar.ejs') %>

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        <%- include('header/topbar.ejs') %>

        <!-- Begin Page Content -->
        <div class="container-fluid">

          <!-- Page Heading -->
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-0 text-gray-800">History</h1>
          </div>

      <!-- DataTales Example -->
      <div class="card shadow mb-4">
			<div class="card-header py-3">
				<h6 class="m-0 font-weight-bold text-primary">History</h6>
			</div>
			<div class="card-body">
				<div class="table-responsive">
					<div class="row">
						<div class="col-3">
							<label for="date" class="form-control-label">Date</label>
							<input type="datetime-local" class="form-control" id="date" name="date">
						</div>
					</div>
					<br>
					<div class="row">
						<div class="col-3">
							<input type="button" class="btn btn-primary" id="buttonFilter" value="Filter">
						</div>
					</div>
					<br>
					<input type="hidden" id="placeId" name="placeId" value="<%= placeId %>">
					<table class="table table-bordered table-hovered" id="tableListHistory" width="100%" cellspacing="0">
						<thead class="thead-dark">
							<tr>
								<th>User Id</th>
								<th>Username</th>
								<th>Check In</th>
								<th>Check Out</th>
							</tr>
						</thead>
					</table>
				</div>
			</div>
        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

      <!-- Footer -->
      <footer class="sticky-footer bg-white">
        <div class="container my-auto">
          <div class="copyright text-center my-auto">
            <span>Copyright &copy; Your Website 2020</span>
          </div>
        </div>
      </footer>
      <!-- End of Footer -->

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fa fa-angle-up"></i>
  </a>
  <%- include('modals/modals.ejs') %>
  <%- include('footer/footer.ejs') %>;